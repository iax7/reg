<p id="notice"><%= notice %></p>

<h1>Guests</h1>

<table>
  <thead>
    <tr>
      <th>Registry</th>
      <th>Name</th>
      <th>Lastname</th>
      <th>Nick</th>
      <th>Is male</th>
      <th>Age</th>
      <th>Relation</th>
      <th>Is pregnant</th>
      <th>Is medicated</th>
      <th>F v1</th>
      <th>F v2</th>
      <th>F v3</th>
      <th>F s1</th>
      <th>F s2</th>
      <th>F s3</th>
      <th>F d1</th>
      <th>F d2</th>
      <th>F d3</th>
      <th>F l1</th>
      <th>F l2</th>
      <th>F l3</th>
      <th>T v1</th>
      <th>T v2</th>
      <th>T s1</th>
      <th>T s2</th>
      <th>T d1</th>
      <th>T d2</th>
      <th>T l1</th>
      <th>T l2</th>
      <th>L v</th>
      <th>L s</th>
      <th>L d</th>
      <th>L l</th>
      <th>L room</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @guests.each do |guest| %>
      <tr>
        <td><%= guest.registry %></td>
        <td><%= guest.name %></td>
        <td><%= guest.lastname %></td>
        <td><%= guest.nick %></td>
        <td><%= guest.is_male %></td>
        <td><%= guest.age %></td>
        <td><%= guest.relation %></td>
        <td><%= guest.is_pregnant %></td>
        <td><%= guest.is_medicated %></td>
        <td><%= guest.f_v1 %></td>
        <td><%= guest.f_v2 %></td>
        <td><%= guest.f_v3 %></td>
        <td><%= guest.f_s1 %></td>
        <td><%= guest.f_s2 %></td>
        <td><%= guest.f_s3 %></td>
        <td><%= guest.f_d1 %></td>
        <td><%= guest.f_d2 %></td>
        <td><%= guest.f_d3 %></td>
        <td><%= guest.f_l1 %></td>
        <td><%= guest.f_l2 %></td>
        <td><%= guest.f_l3 %></td>
        <td><%= guest.t_v1 %></td>
        <td><%= guest.t_v2 %></td>
        <td><%= guest.t_s1 %></td>
        <td><%= guest.t_s2 %></td>
        <td><%= guest.t_d1 %></td>
        <td><%= guest.t_d2 %></td>
        <td><%= guest.t_l1 %></td>
        <td><%= guest.t_l2 %></td>
        <td><%= guest.l_v %></td>
        <td><%= guest.l_s %></td>
        <td><%= guest.l_d %></td>
        <td><%= guest.l_l %></td>
        <td><%= guest.l_room %></td>
        <td><%= link_to 'Show', guest %></td>
        <td><%= link_to 'Edit', edit_guest_path(guest) %></td>
        <td><%= link_to 'Destroy', guest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

